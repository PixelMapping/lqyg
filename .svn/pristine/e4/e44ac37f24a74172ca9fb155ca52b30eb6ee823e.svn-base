.main{width:100%;background:#fff}.assets{background:#fff;padding:24px;min-height:600px}.assets .sucess{text-align:center;padding:200px 0}.assets .sucess .tit{font-size:22px;color:#434343;font-weight:700;margin:35px 0 0}.assets .sucess .des{color:#8c8c8c;margin:24px 0}.assets .card{height:392px;border:1px solid #dedede;border-radius:4px;overflow:hidden;margin-bottom:48px}.assets .card .head{display:flex;justify-content:space-between;background:#f0f0f0;height:48px;align-items:center;padding:0 32px;color:#434343}.assets .card .amount{padding:0 16px;display:flex;flex-direction:column;justify-content:center;height:100px;border-bottom:1px solid #dedede}.assets .card .amount .num{font-size:20px;font-weight:400;color:#1890ff}.assets .card .info{padding-left:32px;border-bottom:1px solid #dedede}.assets .card .info .tit{margin:31px 0 24px;color:#8c8c8c}.assets .card .info .tit img{padding-left:8px}.assets .card .btns{display:flex;align-items:center;justify-content:space-between;padding:0 30px;height:60px}.assets .card .btns span{cursor:pointer}.assets .card .btns span:hover{color:#1890ff;transition:all .2s}.assets .confirm{padding:10px}.assets .confirm .row{margin-bottom:30px}.assets .confirm .row .lt{text-align:right;color:rgba(0,0,0,.85);margin-right:10px}.assets .translate{padding:20px;border:1px solid #d9d9d9;width:320px;color:#f5222d}.assets .total{position:relative}.assets .total .num{position:absolute;left:30%;top:5px}.record{height:100%}.detail .ant-row{margin-bottom:25px}.detail .ant-col{display:flex}.detail .ant-col span:first-child{width:100px;text-align:right;margin-right:15px}.imgs{width:200px;height:200px}.imgs img{width:100%;height:100%}.des{color:#ff4d4f}